extends layout

block content
  link(rel='stylesheet', href='/css/users.css')
  body
    p#title= 'Registered users list:'
    div.list
      for user in users
        a(href='/users/'+user.id)
          span#username= user.username
          span#id= 'id: ' + user.id
    nav.menu
      a(href='/webchat')= 'Webchat'
      a(href='/logout')= 'Logout'
